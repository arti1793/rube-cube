{
  "name": "webpack-typescript-starter",
  "version": "0.0.1",
  "description": "cuberube",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext",
    "test": "jest"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "arti1793",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^25.2.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "7.0.0",
    "@types/offscreencanvas": "^2019.6.2",
    "@types/webgl2": "0.0.5",
    "awesome-typescript-loader": "5.2.0",
    "core-js": "^3.6.5",
    "file-loader": "^6.0.0",
    "jest": "^25.3.0",
    "source-map-loader": "0.2.3",
    "ts-jest": "^25.3.1",
    "tslint": "5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.5.1",
    "webpack": "^4.42.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "three": "^0.115.0"
  }
}
